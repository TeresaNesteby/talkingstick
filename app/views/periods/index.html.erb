<p id="notice"><%= notice %></p>

<h1>Listing Periods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Period</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @periods.each do |period| %>
      <tr>
        <td><%= period.name %></td>
        <td><%= period.period %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

